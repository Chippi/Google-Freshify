{
  "manifest_version": 2,
  "name": "YoloFilter",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "include_globs": [
        "https://www.google.*/search*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "background": {
